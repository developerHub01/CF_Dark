{
  "manifest_version": 3,
  "name": "CF Dark",
  "version": "1.0",
  "description": "CF Dark",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["https://codeforces.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "128": "images/assets/128.png",
      "32": "images/assets/32.png",
      "24": "images/assets/24.png",
      "16": "images/assets/16.png"
    },
    "default_title": "CF Dark"
  },
  "icons": {
    "128": "images/assets/128.png",
    "32": "images/assets/32.png",
    "24": "images/assets/24.png",
    "16": "images/assets/16.png"
  }
}
